.PHONY: all

all: a b

%: subdir/%
	&cp $(inputs) $(outputs)

$(foreach): : foreach c d
	&echo $@ -o $@

subdir/a subdir/b : d c c e
	&echo $@ $(@D) $(@F) -o $@
	&echo $< $(<D) $(<F) -o>>$@
	&echo $^ $(^D) $(^F) -o>>$@
	&cat $& $+ -o >>$@
	&echo  "$(outputs) : $(inputs)" -o>>subdir/b
	&echo  "$(output 2) | $(outputs -1 1) : $(input -1) | $(inputs 2 1)" -o>>subdir/b

%: subdir/%.x
	&echo $* $(*D) $(*F) -o $@
	&cp $(dependencies) $(targets)

subdir/e.x:
	&echo $@ -o $@
